SELECT DISTINCT ?g1 ?g2 ?g3 WHERE {
  VALUES ( ?s1 ?s2 ?s3 ) {
    ( <http://localhost/sparql/swdfood> <http://localhost/sparql/swdfood> <http://localhost/sparql/swdfood> )
  }
  SERVICE ?s1 { ?paper <http://data.semanticweb.org/ns/swc/ontology#isPartOf> <http://data.semanticweb.org/conference/iswc/2008/poster_demo_proceedings> . }
  SERVICE ?s2 { ?paper <http://swrc.ontoware.org/ontology#author> ?p . }
  SERVICE ?s3 { ?p rdfs:label ?n . }
}